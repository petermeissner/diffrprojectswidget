  [25, 547, 576, "Kehrt' ich mein verirrtes Auge", 1],
  [26, 578, 608, "Zur Sonne, als wenn drüber wär'", 1],
  [27, 610, 639, "Ein Ohr, zu hören meine Klage,", 1],
  [28, 641, 661, "Ein Herz, wie mein's,", 1],
