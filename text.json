  [1, 2, 27, "Johann Wolfgang von Goethe", 1],
  [2, 30, 41, "\"Prometheus\"", 1],
  [3, 44, 71, "Bedecke deinen Himmel, Zeus,", 1],
  [4, 73, 88, "Mit Wolkendunst,", 1],
  [5, 90, 116, "Und übe, dem Knaben gleich,", 1],
