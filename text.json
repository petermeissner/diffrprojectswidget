  [19, 414, 427, "Eure Majestät,", 1],
  [20, 429, 446, "Und darbtet, wären", 1],
  [21, 448, 471, "Nicht Kinder und Bettler", 1],
  [22, 473, 494, "Hoffnungsvolle Thoren.", 1],
  [23, 497, 516, "Da ich ein Kind war,", 1],
