  [12, 262, 276, "Um dessen Gluth", 1],
  [13, 278, 295, "Du mich beneidest.", 1],
  [14, 298, 322, "Ich kenne nichts Aermeres", 1],
  [15, 324, 357, "Unter der Sonn', als euch, Götter!", 1],
  [16, 359, 379, "Ihr nähret kümmerlich", 1],
  [17, 381, 396, "Von Opfersteuern", 1],
