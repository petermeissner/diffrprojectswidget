  [30, 697, 708, "Wer half mir", 1],
  [31, 710, 737, "Wider der Titanen Uebermuth?", 1],
  [32, 739, 764, "Wer rettete vom Tode mich,", 1],
  [33, 766, 779, "Von Sklaverey?", 1],
  [34, 781, 817, "Hast du nicht alles selbst vollendet,", 1],
  [35, 819, 838, "Heilig glühend Herz?", 1],
  [36, 840, 865, "Und glühtest jung und gut,", 1],
  [37, 867, 888, "Betrogen, Rettungsdank", 1],
  [38, 890, 915, "Dem Schlafenden da droben?", 1],
  [39, 918, 939, "Ich dich ehren? Wofür?", 1],
  [40, 941, 971, "Hast du die Schmerzen gelindert", 1],
  [41, 973, 989, "Je des Beladenen?", 1],
  [42, 991, 1019, "Hast du die Thränen gestillet", 1],
  [43, 1021, 1039, "Je des Geängsteten?", 1],
  [44, 1041, 1076, "Hat nicht mich zum Manne geschmiedet", 1],
  [45, 1078, 1097, "Die allmächtige Zeit", 1],
  [46, 1099, 1122, "Und das ewige Schicksal,", 1],
  [47, 1124, 1145, "Meine Herrn und deine?", 1],
  [48, 1148, 1164, "Wähntest du etwa,", 1],
  [49, 1166, 1193, "Ich sollte das Leben hassen,", 1],
  [50, 1195, 1212, "In Wüsten fliehen,", 1],
  [51, 1214, 1228, "Weil nicht alle", 1],
  [52, 1230, 1251, "Blüthenträume reiften?", 1],
  [53, 1254, 1283, "Hier sitz' ich, forme Menschen", 1],
  [54, 1285, 1302, "Nach meinem Bilde,", 1],
  [55, 1304, 1338, "Ein Geschlecht, das mir gleich sey,", 1],
  [56, 1340, 1360, "Zu leiden, zu weinen,", 1],
  [57, 1362, 1392, "Zu genießen und zu freuen sich,", 1],
